		"tailwindcss": "^3.3.2",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"unified": "^10.1.2",
		"unplugin-icons": "^0.16.3",
		"vite": "^4.3.9",
		"vitest": "^0.32.2"
